programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite o numero: ")
    leia(numero)
    
    se(numero>0){
    escreva("positivo! :")  //testar se e positivo

    }
    
    se(numero<0){ //testar se o numero e negativo
      escreva("negativo!")
    }
    se(numero==0){  //testar se e zero
    escreva("zero!")

    }
  }
}
